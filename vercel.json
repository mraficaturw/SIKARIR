{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/img/$1"
        },
        {
            "src": "/fonts/(.*)",
            "dest": "/public/fonts/$1"
        },
        {
            "src": "/lib/(.*)",
            "dest": "/public/lib/$1"
        },
        {
            "src": "/scss/(.*)",
            "dest": "/public/scss/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/public/img/favicon.ico"
        },
        {
            "src": "/favicon.png",
            "dest": "/public/img/favicon.ico"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    },
    "outputDirectory": "public"
}